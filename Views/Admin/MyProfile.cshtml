@{
    ViewBag.Title = "MyProfile";
    Layout = "~/Views/Shared/MasterAdmin.cshtml";
}

    <div class="col-sm-12" style="background-image: url(/Content/ForAdmin/Pback.png);min-height:372px;padding:0px;background-repeat:no-repeat;background-size:100% 100%;"><div class="row">
    <div class="col-sm-12 text-left" style="min-height:40px;font-size:26px;font-family:Algerian;color:ThreeDDarkShadow;margin-left:50px;padding-top:7px;"> My Profile</div><br /><br />
    <div class="col-sm-12" style="min-height:;">
        <br /><br />
        <div class="col-sm-2">
            <img src="~/Content/ForAdmin/IMG20221231171803.jpg" class="img img-circle hvr-box-shadow-outset" height="150px" width="90%" />
        </div>
        <div class="col-sm-10">
            <div class="col-sm-5">
                <span class="a">Name : </span><span class="b">@ViewBag.name </span>
            </div><div class="col-sm-5">  <span class="a">Gender : </span><span class="b">@ViewBag.gender </span></div> <br /><br />
            <div class="col-sm-5">
                <span class="a">E-mail : </span><span class="b">@ViewBag.email </span>
            </div><div class="col-sm-5"><span class="a">Mobile :  </span><span class="b">@ViewBag.mobile </span></div> <br /><br />
            <div class="col-sm-5">
                <span class="a">DOB : </span><span class="b">@ViewBag.dob </span>
            </div><div class="col-sm-5">  <span class="a">Father's Name : </span><span class="b">@ViewBag.fn</span></div><br /><br />
            <div class="col-sm-5">
                <span class="a">State : </span><span class="b">@ViewBag.state </span>
            </div><div class="col-sm-5"><span class="a">Qualification : </span><span class="b">@ViewBag.qualification </span></div> <br /><br />
            <br />
            <div class="col-sm-5">
                <a href="~/Admin/EditProfile" class="btn btn-info " style="font-weight:bold;font-size:17px;">Edit Profile</a>
            </div>
        </div>
    </div>
</div></div>